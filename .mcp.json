{
  "mcpServers": {
    "serena": {
      "type": "stdio",
      "command": "uvx",
      "args": [
        "--from",
        "git+https://github.com/oraios/serena",
        "serena",
        "start-mcp-server",
        "--context",
        "claude-code",
        "--project",
        ".",
        "--enable-web-dashboard",
        "false"
      ],
      "env": {}
    },
    "codex": {
      "type": "stdio",
      "command": "codex",
      "args": ["-m", "gpt-5.3-codex", "mcp-server"],
      "env": {
        "CODEX_HOME": ".codex"
      }
    }
  }
}
